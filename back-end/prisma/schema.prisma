generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Product {
  id             String      @id @default(uuid())
  name           String   @db.VarChar(90)
  code           Int
  expirationDate DateTime
  category       String
  quantity       Int
  price          Float    @db.Float
  description    String

  @@map("product")
}
